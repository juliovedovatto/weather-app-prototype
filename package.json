{
  "name": "weather-webapp",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit -p tsconfig.json",
    "prepare": "husky",
    "format": "prettier --write .",
    "test": "LC_ALL=en_US.UTF-8 vitest run",
    "test:watch": "LC_ALL=en_US.UTF-8 vitest",
    "deploy:preview": "vercel --confirm",
    "deploy": "vercel --prod --confirm"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-better-tailwindcss": "^3.7.7",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-vue": "^10.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.43.0",
    "vercel": "^47.0.7",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.6"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.87.1",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "vue": "^3.5.21"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20.x"
  }
}
